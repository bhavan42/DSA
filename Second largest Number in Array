#include <iostream>
using namespace std;
int main ()
{
    int n;
    cin >> n;
    
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin >> arr[i];
    }
    
    int max, smax;
    max=arr[0];
    smax= -1;
    
    for(int i=1;i<n;i++)
    {
        if(max < arr[i])
        {
            max=arr[i];
            smax=arr[i-1];
        }
        else if(smax < arr[i] && arr[i]!= max)
        {
            smax=arr[i];
        }
    }
    cout << "Smax = " << smax;
    return 0;
}
